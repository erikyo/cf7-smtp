{
    "name": "wpbp/wordpress-plugin-boilerplate-powered",
    "description": "Wordpress Plugin Boilerplate with steroids!",
    "license": "GPL-3.0-or-later",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Erik Golinelli",
            "email": "erik@codekraft.it",
            "homepage": "https://author-url.test"
        }
    ],
    "homepage": "https://github.com/WPBP/WordPress-Plugin-Boilerplate-Powered",
    "require": {
        "php": ">=7.4",
        "composer/installers": "1.x",
        "decodelabs/tagged": "^0.11.9",
        "inpsyde/wp-context": "1.3.*",
        "yoast/i18n-module": "3.1.*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.6"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ayecode/wp-super-duper"
        },
        {
            "type": "package",
            "package": {
                "name": "mte90/i18n-module",
                "version": "3.1.0",
                "autoload": {
                    "classmap": [
                        "src/"
                    ]
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/mte90/i18n-module.git",
                    "reference": "master"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "cf7_smtp\\Backend\\": "backend/",
            "cf7_smtp\\Core\\": "core/",
            "cf7_smtp\\Engine\\": "engine/",
            "cf7_smtp\\Rest\\": "rest/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpro/grumphp": true
        }
    }
}
